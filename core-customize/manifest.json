{
    "commerceSuiteVersion": "2211-jdk21.6",
    "extensionPacks": [],
    "solr": {
        "location": "solr",
        "solrVersion": "9.10"
    },
    "extensions": [],
    "properties": [],
    "useCloudExtensionPack": false,
    "useConfig": {
        "extensions": {
            "location": "hybris/config/localextensions.xml",
            "exclude": [ "cxdevproxy" ]
        },
        "properties": [
            {
                "location": "hybris/config/cloud/common.properties"
            },
            {
                "persona": "development",
                "location": "hybris/config/cloud/persona/development.properties"
            },
            {
                "persona": "staging",
                "location": "hybris/config/cloud/persona/staging.properties"
            },
            {
                "persona": "production",
                "location": "hybris/config/cloud/persona/production.properties"
            },
            {
                "aspect": "api",
                "location": "hybris/config/cloud/aspect/api.properties"
            },
            {
                "aspect": "backgroundProcessing",
                "location": "hybris/config/cloud/aspect/backgroundprocessing.properties"
            },
            {
                "aspect": "backoffice",
                "location": "hybris/config/cloud/aspect/backoffice.properties"
            }
        ],
        "tests": {
            "extensions": ["sapcommercetoolkit", "sapcxbackoffice", "sapcxreporting", "sapcxsearch", "sapcxsinglesignon"],
            "annotations": ["UnitTests", "IntegrationTests"],
            "packages": ["tools.sapcx.commerce.*"]
        }
    },
    "aspects": [
        {
            "name": "api",
            "properties": [],
            "webapps": [
                {
                    "contextPath": "/hac",
                    "name": "hac"
                },
                {
                    "contextPath": "/occ",
                    "name": "commercewebservices"
                },
                {
                    "contextPath": "/medias",
                    "name": "mediaweb"
                },
                {
                    "contextPath": "/authorizationserver",
                    "name": "authorizationserver"
                },
                {
                    "contextPath": "/resourceserver",
                    "name": "resourceserver"
                }
            ]
        },
        {
            "name": "backgroundProcessing",
            "properties": [],
            "webapps": [
                {
                    "contextPath": "/hac",
                    "name": "hac"
                },
                {
                    "contextPath": "/odata2webservices",
                    "name": "odata2webservices"
                },
                {
                    "contextPath": "/authorizationserver",
                    "name": "authorizationserver"
                },
                {
                    "contextPath": "/resourceserver",
                    "name": "resourceserver"
                }
            ]
        },
        {
            "name": "backoffice",
            "webapps": [
                {
                    "contextPath": "/hac",
                    "name": "hac"
                },
                {
                    "contextPath": "/backoffice",
                    "name": "backoffice"
                },
                {
                    "contextPath": "/medias",
                    "name": "mediaweb"
                },
                {
                    "contextPath": "/authorizationserver",
                    "name": "authorizationserver"
                },
                {
                    "contextPath": "/resourceserver",
                    "name": "resourceserver"
                }
            ]
        }
    ]
